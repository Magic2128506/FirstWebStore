@model IEnumerable<FirstWebStore.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
    <table>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.ID</td>
                <td>@employee.FirstName</td>
                <td>@employee.SurName</td>
                <td>@employee.Patronymic</td>
                <td>@employee.Age</td>
                <td>
                    <a href="/Home/Employee/@employee.ID">Открыть</a>
                </td>
            </tr>
        }
    </table>
</body>
</html>
